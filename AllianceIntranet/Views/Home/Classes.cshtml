@model IEnumerable<CEClass>
@{ }
<h1>Classes Submitted</h1>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Time</th>
            <th>Instructor</th>
            <th>Type</th>
            <th>Class Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ceClass in Model)
        {
            <tr>
                <td scope="row">@ceClass.Date</td>
                <td>@ceClass.Time</td>
                <td>@ceClass.Instructor</td>
                <td>@ceClass.Type</td>
                <td>@ceClass.ClassTitle</td>
            </tr>
        }
    </tbody>
</table>
